<script>
    $(function () {

        Morris.Bar({
            parseTime: false,
            element: 'morris-bar-chart',
            data: [
                {% for monthAttendanceInfo in  attendanceInfo %}
                {
                    Username: '{{ monthAttendanceInfo.user.first_name }}',  Checkin: '{{ monthAttendanceInfo.totalCheckIn }}'
                },
                {% endfor %}
            ],

            xkey: 'Username',
            ykeys: ['Checkin'],
            labels: ['Checkin']
        });
    });
</script>

<script>
    Morris.Line({
        element: 'morris-line-chart',
        data: [
            { y: '2006', a: 100, b: 90 },
            { y: '2007', a: 75,  b: 65 },
            { y: '2008', a: 50,  b: 40 },
            { y: '2009', a: 75,  b: 65 },
            { y: '2010', a: 50,  b: 40 },
            { y: '2011', a: 75,  b: 65 },
            { y: '2012', a: 100, b: 90 }
        ],
        xkey: 'y',
        ykeys: ['a', 'b'],
        labels: ['Series A', 'Series B']
    });
</script>

