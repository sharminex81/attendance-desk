{
  "name": "sharminex81/attendance-desk",
  "description": "This is a Attendance management system for employee of a organization",
  "keywords": [
    "Attendance",
    "Office Attendance"
  ],
  "homepage": "https://github.com/sharminex81/attendance-desk",
  "authors": [
    {
      "name": "Sharmin Shanta",
      "email": "shantaex81@gmail.com",
      "homepage": "https://sharminshanta.com/"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "slim/slim": "^3.1",
    "slim/php-view": "^2.0",
    "monolog/monolog": "^1.17",
    "slim/twig-view": "^2.1",
    "slim/flash": "^0.1.0",
    "illuminate/database": "*",
    "illuminate/events": "^5.3",
    "ircmaxell/password-compat": "^1.0",
    "mandrill/mandrill": "^1.0",
    "vlucas/valitron": "1.3.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">=4.8 < 6.0"
  },
  "autoload": {
    "psr-4": {
      "Lib\\": "lib/",
      "Model\\": "model/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "start": "php -S 0.0.0.0:8080 -t public public/index.php",
    "test": "phpunit"
  }
}
